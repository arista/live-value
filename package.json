{
  "name": "live-value",
  "version": "0.0.4",
  "description": "A simple mechanism for connecting React components to changing model data",
  "keywords": [
    "typescript",
    "template"
  ],
  "license": "MIT",
  "author": "Nathan Abramson",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettier": "npx prettier --write src/**/*.{ts,tsx,css,scss,html,json,js}",
    "tsc": "npx tsc",
    "tsc-watch": "npx tsc -w",
    "build": "npx rollup -c rollup.config.js",
    "build-watch": "npx rollup -w -c rollup.config.js",
    "test": "npx jest",
    "test-watch": "npx jest --watch",
    "lint": "npx eslint src/**/*.{js,ts,tsx}",
    "start": "concurrently npm:test-watch npm:tsc-watch npm:build-watch"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.35.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "rootDir": "./src",
    "testMatch": [
      "**/*.spec.ts",
      "**/*.spec.js"
    ]
  }
}
